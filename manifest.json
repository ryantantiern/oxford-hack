{
  "name": "Bad word deleter",
  "description": "Removes all trigger words from webpage",
  "version": "2.0",

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["replacewords.js"]
  }
],

"permissions": [
          "storage"
        ],

"options_page": "settings.html",

  "manifest_version": 2
}
